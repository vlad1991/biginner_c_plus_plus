#include <iostream>

int main(int argc, const char * argv[]) {
    // insert code here...
    int x = 8;
    int y = 6;
    std::cout << std::endl;
    std::cout << x-y << "  " << x*y << x+y;
    std::cout << std::endl;
    return 0;
}

